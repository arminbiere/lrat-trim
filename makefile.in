COMPILE=@COMPILE@
all: lrat-trim
lrat-trim: lrat-trim.c makefile
	$(COMPILE) -o $@ $<
clean:
	rm -f lrat-trim makefile
	rm -f test/*/*.log* test/*/*.err* test/*/*.lrat[12]
	rm lrat-trim.gcda lrat-trim.gcno lrat-trim.c.gcov
coverage: test
	gcov lrat-trim.c
format:
	clang-format lrat-trim.c
test: lrat-trim
	test/parse/run.sh
	test/trim/run.sh
	test/regr/run.sh
.PHONY: all clean coverage format test
